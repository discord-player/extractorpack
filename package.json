{
  "name": "extractorpack",
  "version": "0.0.1-beta",
  "description": "A collection of Collection of extractors for discord-player and sonata",
  "main": "index.js",
  "repository": "https://github.com/discord-player/extractorpack.git",
  "author": "discord-player team",
  "license": "Creative Commons",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo build --filter !@extractorpack/tsconfig",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "tsup": "^8.1.0",
    "turbo": "^2.0.6",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.3.1"
}
